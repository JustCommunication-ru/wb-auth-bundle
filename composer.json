{
    "name": "justcommunication-ru/auth-bundle",
    "description": "Security auth bundle",
    "type": "symfony-bundle",
    "keywords": ["security", "autentification", "user", "registration"],
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.15",
        "justcommunication-ru/func-bundle": "^0.1.3",
        "symfony/config": ">=5.4 ^6.2",
        "symfony/dependency-injection": ">=5.4 ^6.2",
        "symfony/framework-bundle": ">=5.4 ^6.2",
        "symfony/http-client": ">=5.4 ^6.2",
        "symfony/http-kernel": ">=5.4 ^6.2",
        "symfony/security-bundle": ">=5.4 ^6.2",
        "symfony/security-http": ">=5.4 ^6.2",
        "symfony/twig-bundle": ">=5.4 ^6.2",
        "symfony/yaml": ">=5.4 ^6.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:JustCommunication-ru/func-bundle.git"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JustCommunication\\AuthBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JustCommunication\\AuthBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/browser-kit": "^6.2",
        "symfony/phpunit-bridge": "^6.2"
    }
}
